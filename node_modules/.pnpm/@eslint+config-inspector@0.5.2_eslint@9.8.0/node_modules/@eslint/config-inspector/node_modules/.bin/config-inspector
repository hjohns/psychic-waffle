#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules/@eslint/config-inspector/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules/@eslint/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules/@eslint/config-inspector/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules/@eslint/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/@eslint+config-inspector@0.5.2_eslint@9.8.0/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.mjs" "$@"
else
  exec node  "$basedir/../../bin.mjs" "$@"
fi
