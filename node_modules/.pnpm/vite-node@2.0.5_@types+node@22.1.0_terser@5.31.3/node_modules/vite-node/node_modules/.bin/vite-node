#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/vite-node@2.0.5_@types+node@22.1.0_terser@5.31.3/node_modules/vite-node/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/vite-node@2.0.5_@types+node@22.1.0_terser@5.31.3/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/vite-node@2.0.5_@types+node@22.1.0_terser@5.31.3/node_modules/vite-node/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/vite-node@2.0.5_@types+node@22.1.0_terser@5.31.3/node_modules:/Users/hjohns/workspace/kurrawong/v4/psychic-waffle/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
